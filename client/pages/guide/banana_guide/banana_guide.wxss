.weui-search-bar {
    position:relative;
    padding:6px 10px;
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    box-sizing:border-box;
    background-color:#EFEFF4;
    border-top:0 solid #D7D6DC;
    border-bottom:0 solid #D7D6DC;
    border-radius:4px;
}

.search-log{
    border:1px solid #eee !important;
    display:inline-block !important;
    margin-left:10px !important;
    margin-top:5px !important;
    border-radius:5px;
    padding-left:14px;
    padding-right:14px;
    box-sizing:border-box;
    text-align:center;
    text-decoration:none;
    line-height:2.55555556;
}
 
.clear-search-log{
    text-align: center;
}

.kind-list{
  display: block;
  padding: 20rpx;
}

.kind-list_item{
  padding-left:0rpx;
  display: block;
  float: left;
  width:350rpx;
  height:330rpx;
}

.plant-panel{
  padding:10rpx;
  color: #999999;
  font-size: 26rpx;
  
}

.plant-box {
  padding: 10rpx;
  position: relative;
  display: block;
  box-sizing: border-box;
}

.plant-image {
  display:block;
}

.plant-box-image {
  width: 310rpx;
  height: 250rpx;
}

.plant-box-title {
  position:relative;
  text-align: center;
  display: block;
  font-size: 26rpx;
}


